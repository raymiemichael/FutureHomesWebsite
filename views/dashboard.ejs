<div style="float:left; width:30%;">
    <div class="container">

      <h2 class="cta-heading">Messages</h2>

      <% for(var i = 0; i < messages.length; i++ ) { %>
      <div class="bs-calltoaction bs-calltoaction-default">
        <div class="row">
          <div class="col-md-4 cta-contents">
            <h1 class="cta-title"><%=messages[i].sender_name %> </h1>
            <div class="cta-desc">
              <p><%=messages[i].sender_email %></p>
              <p><%=messages[i].message %></p>
            </div>
          </div>
        </div>
      </div>
      <% } %>

    </div>
</div>

<div style="float:right; width:70%;">
    <div class="container">

        <div style=display:inline-block>

      <h2 class="cta-heading">Your Listings</h2>
        </div>
        <div style=display:inline-block>
      <!-- Button that connects you to upload page -->
      <form method ="GET">
        <input style="text-align: right;" type="submit" class="btn btn-block btn-lg btn-primary" value="Upload" formaction="/fa17g10/upload"></input>
      </form>


        </div>

      <% for(var i = 0; i < listings.length; i++ ) { %>
      <div class="bs-calltoaction bs-calltoaction-default">
        <div class="row">
          <div class="col-md-4 cta-contents">
            <h1 class="cta-title"><%=listings[i].street %></h1>
            <div class="cta-desc">
              <a class="btn btn-primary" href="/fa17g10/details/<%= listings[i].id %>">View Details</a>
            </div>
          </div>
        </div>
      </div>
      <% } %>

    </div>

</div>




